; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Jordan's Useful Godot Tools"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.png"

[autoload]

DebugActions="*res://Scripts/Autoloads/debug_actions.gd"
PauseManager="*res://Scripts/Autoloads/pause_manager.gd"
PhantomCameraManager="*res://addons/phantom_camera/scripts/managers/phantom_camera_manager.gd"

[editor]

script/templates_search_path="res://Scripts/Script Templates"

[editor_plugins]

enabled=PackedStringArray("res://addons/phantom_camera/plugin.cfg")
